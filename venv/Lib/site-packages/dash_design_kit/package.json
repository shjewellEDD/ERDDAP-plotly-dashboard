{
  "name": "dash_design_kit",
  "version": "1.5.4",
  "description": "Beautifully designed and thoroughly tested, Dash Design Kit is a premium set of building blocks, editable templates, and themes for professional Dash app development. No CSS required.",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve ./webpack.serve.config.js --open",
    "postbuild:all": "es-check es5 dash_design_kit/*.js",
    "build:js": "webpack --mode production",
    "build:py": "dash-generate-components ./src/lib/components dash_design_kit",
    "build:py_and_r": "dash-generate-components ./src/lib/components dash_design_kit -p package-info.json && dash-generate-components ./src/lib/components dash_design_kit -p package-info.json --r-prefix='ddk'",
    "build:py-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py)",
    "build:all": "npm run build:js && npm run build:py_and_r",
    "build:all-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:all)",
    "build:package": "npm run build:all && python build.py",
    "lint": "eslint src"
  },
  "author": "Chris Parmer <chris@plotly.com>",
  "maintainer": "Ryan Patrick Kyle <ryan@plotly.com>",
  "license": "Commercial",
  "dependencies": {
    "chroma-js": "^2.0.1",
    "fast-isnumeric": "^1.1.2",
    "file-saver": "^1.3.8",
    "fscreen": "^1.0.2",
    "is_js": "^0.9.0",
    "ramda": "^0.26.1",
    "rc-slider": "^8.4.0",
    "react-beautiful-dnd": "^6.0.2",
    "react-color": "^2.14.1",
    "react-copy-to-clipboard": "^5.0.1",
    "react-popout": "^1.0.1",
    "react-resize-detector": "^4.2.3",
    "react-select": "^1.2.1",
    "react-toggle": "^4.0.2",
    "react-transition-group": "^1.2.1",
    "tinycolor2": "^1.4.1",
    "url-loader": "^1.1.2",
    "use-force-update": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@plotly/dash-component-plugins": "^1.2.0",
    "@plotly/webpack-dash-dynamic-import": "^1.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copyfiles": "^2.0.0",
    "core-js": "^3.6.4",
    "css-box-shadow": "^1.0.0-3",
    "css-loader": "^0.28.11",
    "dash-core-components": "1.10.2",
    "dash-table": "4.9.0",
    "es-check": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "exec-sh": "^0.3.2",
    "npm": "^6.1.0",
    "raw-loader": "^4.0.0",
    "react": "16.13.0",
    "react-docgen": "^5.3.0",
    "react-dom": "16.13.0",
    "style-loader": "^0.21.0",
    "terser-webpack-plugin": "^2.3.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-serve": "^2.0.3"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
